        "overrides": []
      },
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 0,
        "y": 1
      },
      "id": 2,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "showPercentChange": false,
        "textMode": "auto",
        "wideLayout": true
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "floor(sum(increase(mcp_tool_invocations_total{namespace=~\"$namespace\"}[24h])) or vector(0))",
          "legendFormat": "Total Operations",
          "refId": "A"
        }
      ],
      "title": "Total Operations Today",
      "type": "stat",
      "description": "Total number of actions performed by AI agents on your behalf today."
